<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  
  <appSettings>
    <!-- HTTPサーバ�E設宁E-->
    <add key="DefaultPort" value="8080" />
    <add key="MaxPortRange" value="10" />
    <add key="ServerTimeout" value="60000" />
    
    <!-- SightEdit拡張機�E設宁E-->
    <add key="ExtensionId" value="chibfgpnajlchhljdojcpmamhplnogcp" />
    <add key="ExtensionPath" value="/editor.html" />
    
    <!-- ログ設宁E-->
    <add key="EnableLogging" value="true" />
    <add key="LogLevel" value="Info" />
    
    <!-- セキュリチE��設宁E-->
    <add key="AllowedOrigins" value="*" />
